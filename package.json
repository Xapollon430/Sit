{
  "name": "SitterScape",
  "version": "1.0.0",
  "description": "A platform for pet sitters and owners to meet",
  "main": "server.js",
  "scripts": {
    "client:dev": "webpack-dev-server --env.ENV=development",
    "client:build": "webpack --mode production --env.ENV=production",
    "server:dev": "nodemon --exec babel-node src/server/server.js",
    "server:build": "babel src/server --copy-files --out-dir prod-bundle",
    "deploy" : "git add . && git commit -m \"predeploy\" && git push heroku master"
  },
  "repository": {},
  "keywords": [
    "pet",
    "animal",
    "sitter"
  ],
  "author": "Vehbi Karaagac",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "axios": "^0.21.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "formik": "^2.2.1",
    "graphql": "^15.4.0",
    "heroku": "^7.46.2",
    "jsonwebtoken": "^8.5.1",
    "leaflet": "^1.7.1",
    "mongoose": "^5.10.11",
    "react": "^17.0.1",
    "react-animated-burgers": "^1.2.8",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-loader": "^8.1.0",
    "html-webpack-plugin": "^4.5.0",
    "nodemon": "^2.0.6",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
