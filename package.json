{
  "name": "Sit",
  "version": "1.0.0",
  "description": "A platform for pet sitters and owners to meet",
  "main": "server.js",
  "scripts": {
    "client:dev": "webpack-dev-server --config webpack.client.config.js",
    "client:build": "webpack --mode production --config webpack.client.config.js",
    "server:dev": "nodemon --exec babel-node src/server/server.js",
    "server:build": "babel src/server --copy-files --out-dir prod-bundle"
  },
  "repository": {},
  "keywords": [
    "pet",
    "animal",
    "sitter"
  ],
  "author": "Vehbi Karaagac",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "heroku": "^7.42.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.11",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "styled-components": "^5.1.1",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "nodemon-webpack-plugin": "^4.3.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  }
}
